{
  "name": "stylelint-processor-styled-components",
  "version": "0.2.1",
  "description": "A stylelint processor for styled-components",
  "main": "lib/index.js",
  "scripts": {
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --list-different --print-width 100 --tab-width 2 --no-semi --single-quote '{src/**/*.{js,jsx},test/*.{js,jsx}}'",
    "prettier": "prettier --write --print-width 100 --tab-width 2 --no-semi --single-quote '{src/**/*.{js,jsx},test/*.{js,jsx}}'",
    "precommit": "lint-staged",
    "test": "jest test/*.test.js",
    "build": "babel src --out-dir lib",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mxstbr/stylelint-processor-styled-components.git"
  },
  "keywords": [
    "stylelint",
    "processor",
    "stylelint-processor",
    "styled-components",
    "lint"
  ],
  "author": "Max Stoiber",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mxstbr/stylelint-processor-styled-components/issues"
  },
  "homepage": "https://github.com/mxstbr/stylelint-processor-styled-components#readme",
  "greenkeeper": {
    "ignore": [
      "eslint",
      "eslint-plugin-import",
      "typescript"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.7.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.1",
    "lodash": "^4.15.0",
    "prettier": "^1.4.4",
    "stylelint": "^8.0.0"
  },
  "dependencies": {
    "babel-traverse": "^6.16.0",
    "babylon": "^6.12.0",
    "typescript": "~2.3.2",
    "typescript-eslint-parser": "^4.0.0"
  }
}
